<%- include('header'); -%>
 
    <section class="hero-section">
        <div class="background image" style=
        "background-image: url('images/bg-Typing_On_Keyboard.jpg');"></div>

        <div class="background" data-parallax="-0.5" data-parallax-type="elem"
        data-vide-bg="images/Typing_On_Keyboard"></div>

        <div class="content">
            <div class="content-inner">
			
			<h1><strong>Creative</strong> development</h1>
			
			<p>We offer <span
data-typed-str="code|design|technology|creativity|beauty" data-typed-repeat="yes" data-typed-cursor="yes" class=""></span><span class="typed-cursor">|</span> for your projects</p>

                <p><a class="btn btn-white-primary text-uppercase" href=
                "portfolio"><%= lingua.portfolio %></a></p>
            </div>
        </div>

        <div class="discover">
            <a class="discover-btn" data-toggle="smoothscroll" href=
            "#about"><%= lingua.discover %><i class="entypo down-open-big"></i></a>
        </div>
    </section>

    <section class="container-wrap about" id="about">
        <div class="container big-padding">
            <h2><%= lingua.about_title %></h2>

            <p><%= lingua.amazing %></p>

            <nav>
                <a class="btn btn-default text-uppercase" href="blog"><%= lingua.blog %></a>
                <a class="btn btn-default text-uppercase" href="contact"><%= lingua.contact %></a>
            </nav>
        </div>
    </section>

