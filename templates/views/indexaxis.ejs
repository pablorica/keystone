    <section class="container-wrap secondary-bg">
        <div class="container big-padding">
            <div class="text-center">
                <h2><%= lingua.meet_the_team %></h2>

                <p class="font-italic"><%= lingua.about_content %></p>
            </div>

            <ul class=
            "people-list big-margin block-grid-xs-2 block-grid-sm-4 nogutter">
                
                <% for(var i=0; i<data.team.length; i++) {%>
                <li>
                    <img alt="" class="image" src="<%= data.team[i].photo.secure_url %>">

                    <div class="overlay">
                        <h3 class="name"><a href="team/<%= data.team[i].slug %>"><%= data.team[i].name.first %> <%=  data.team[i].name.last %></a> <small><%= data.team[i].position %></small></h3>

                        <ul class="contacts">

											<%if (data.team[i].facebook.length > 0) { %>
                                            <li><a href="<%= data.team[i].facebook %>" target="_blank">Facebook</a></li>
                                            <% } %>
                                            <%if (data.team[i].tumblr.length > 0) { %>
                                            <li><a href="<%= data.team[i].tumblr %>" target="_blank">Tumblr</a></li>
                                            <% } %>
                                            <%if (data.team[i].twitter.length > 0) { %>
                                            <li><a href="<%= data.team[i].twitter %>" target="_blank">Twitter</a></li>
                                            <% } %>
                                            <%if (data.team[i].linkedin.length > 0) { %>
                                            <li><a href="<%= data.team[i].linkedin %>" target="_blank">Linkedin</a></li>
                                            <% } %>
                                            <%if (data.team[i].google.length > 0) { %>
                                            <li><a href="<%= data.team[i].google %>" target="_blank">Google</a></li>
                                            <% } %>
                                            <%if (data.team[i].github.length > 0) { %>
                                            <li><a href="<%= data.team[i].github %>" target="_blank">Github</a></li>
                                            <% } %>
    
                        </ul>
                    </div>
                </li>
				<% } %>
            </ul>
        </div>
    </section>

